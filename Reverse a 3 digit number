 Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int x;
        int y;
        int z;
        int a;
        int b;
        x=n%10;
        a=n-x;
        z=a/10;
        y=z%10;
        b=z-y;
        b=b/10;
        System.out.print(x);
        System.out.print(y);
        System.out.print(b);
